{
    "manifest_version": 3,
    "name": "PXI Fusion",
    "version": "1.0",
    "description": "An extension for PXI Fusion with fancy UI, Prodigy game launcher, and other utilities.",
    "permissions": [
      "activeTab",
      "scripting",
      "declarativeNetRequest"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules.json"
        }
      ]
    },
    "web_accessible_resources": [
      {
        "resources": ["icons/yt.png", "icons/dis.png"],
        "matches": ["*://*/*"]
      }
    ]
  }
  